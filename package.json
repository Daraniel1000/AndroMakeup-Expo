{
  "name": "andromakeup-expo",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "APP_VARIANT=production expo start",
    "dev": "set APP_VARIANT='development' | expo start --dev-client",
    "android": "APP_VARIANT=development expo start --android",
    "help": "eas build --help",
    "build-dev": "eas build --profile development --platform android",
    "build-prev": "eas build --profile preview --platform android",
    "build-prod": "eas build --profile production --platform android",
    "postinstall": "patch-package",
    "devtools": "react-devtools",
    "patch": "patch-package expo-face-detector | patch-package expo-camera | patch-package expo-modules-core | patch-package react-native-expandable-fab"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "dotenv": "^16.0.1",
    "expo": "~45.0.0",
    "expo-asset": "~8.5.0",
    "expo-camera": "~12.2.0",
    "expo-constants": "~13.1.1",
    "expo-dev-client": "~0.9.6",
    "expo-face-detector": "~11.2.0",
    "expo-font": "~10.1.0",
    "expo-linking": "~3.1.0",
    "expo-splash-screen": "~0.15.1",
    "expo-status-bar": "~1.3.0",
    "expo-system-ui": "~1.2.0",
    "expo-web-browser": "~10.2.1",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-expandable-fab": "^1.1.2",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "~3.11.1",
    "react-native-svg": "12.3.0",
    "react-native-web": "0.17.7",
    "react-native-wheel-color-picker": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.66.13",
    "eas-cli": "^0.53.0",
    "jest": "^26.6.3",
    "jest-expo": "~45.0.0",
    "react-devtools": "^4.24.7",
    "react-test-renderer": "17.0.2",
    "typescript": "~4.3.5"
  },
  "private": true
}
